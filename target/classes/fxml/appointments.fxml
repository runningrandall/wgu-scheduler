<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.randalladams.scheduler.controllers.AppointmentController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="%appointments.header">
         <font>
            <Font size="24.0" />
         </font></Label>
      <TableView id="appointmentsTable" fx:id="appointmentsTable" layoutX="13.0" layoutY="58.0" prefHeight="342.0" prefWidth="575.0" />
      <ButtonBar layoutX="194.0" layoutY="18.0" prefHeight="40.0" prefWidth="392.0">
        <buttons>
            <Button fx:id="editAppointmentBtn" mnemonicParsing="false" onAction="#editAppointment" prefHeight="25.0" text="%appointments_table.actions.edit" />
            <Button fx:id="createAppointmentBtn" mnemonicParsing="false" onAction="#createAppointment" text="%appointments_table.actions.new" />
            <SplitMenuButton fx:id="filterButton" mnemonicParsing="false" prefHeight="25.0" text="%appointments_table.actions.filter" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
