<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.randalladams.scheduler.controllers.AppointmentController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="%appointments.header">
         <font>
            <Font size="24.0" />
         </font></Label>
      <TableView id="appointmentsTable" fx:id="appointmentsTable" layoutX="14.0" layoutY="49.0" prefHeight="342.0" prefWidth="575.0" />
      <Button fx:id="createAppointmentBtn" layoutX="465.0" layoutY="19.0" mnemonicParsing="false" onAction="#createAppointment" text="%appointments_table.actions.new" />
      <Button fx:id="editAppointmentBtn" layoutX="342.0" layoutY="19.0" mnemonicParsing="false" onAction="#editAppointment" text="%appointments_table.actions.edit" />
   </children>
</AnchorPane>
